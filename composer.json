{
    "name": "antriver/laravel-notification-utils",
    "description": "Helpers for sending/saving/loading notifications using Laravel's built-in notification function.",
    "authors": [
        {
            "name": "Anthony Kuske",
            "email": "anthonykuske@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Antriver\\LaravelNotificationUtils\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/antriver/laravel-site-utils.git"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "antriver/laravel-site-utils": "dev-master",
        "laravel/framework": "^5.6",
        "tmd/laravel-repositories": "^2.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
